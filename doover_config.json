{
    "generator_control": {
        "name": "generator_control",
        "display_name": "Generator Control",
        "type": "DEV",
        "visibility": "PUB",
        "allow_many": true,
        "description": "A Doover App to Control Generators",
        "long_description": "README.md",
        "depends_on": [
            "platform_interface"
        ],
        "owner_org_key": "",
        "image_name": "ghcr.io/getdoover/generator_control:main",
        "container_registry_profile_key": "",
        "build_args": "--platform linux/amd64,linux/arm64",
        "config_schema": {
            "$schema": "https://json-schema.org/draft/2020-12/schema",
            "$id": "",
            "title": "Application Config",
            "type": "object",
            "properties": {
                "display_name": {
                    "title": "Display Name",
                    "x-name": "display_name",
                    "x-hidden": false,
                    "type": "string",
                    "description": "The display name for the small motor control application.",
                    "default": "Engine"
                },
                "run_command_pin": {
                    "title": "Run Command Pin",
                    "x-name": "run_command_pin",
                    "x-hidden": false,
                    "type": "integer",
                    "description": "The output pin is used to control the generator",
                    "default": 0,
                    "minimum": 0
                },
                "run_sense_pins": {
                    "title": "Run Sense Pins",
                    "x-name": "run_sense_pins",
                    "x-hidden": false,
                    "type": "array",
                    "description": "If any of these pins are high, the generator is running",
                    "items": {
                        "title": "Input Pin",
                        "x-name": "input_pin",
                        "x-hidden": false,
                        "type": "integer",
                        "description": "Input Pin"
                    }
                },
                "warmup_time": {
                    "title": "Warmup Time",
                    "x-name": "warmup_time",
                    "x-hidden": false,
                    "type": "integer",
                    "description": "The time in seconds to warm up the generator",
                    "default": 60,
                    "minimum": 0
                },
                "cooldown_time": {
                    "title": "Cooldown Time",
                    "x-name": "cooldown_time",
                    "x-hidden": false,
                    "type": "integer",
                    "description": "The time in seconds to cool down the generator",
                    "default": 30,
                    "minimum": 0
                },
                "error_timeout": {
                    "title": "Error Timeout",
                    "x-name": "error_timeout",
                    "x-hidden": false,
                    "type": "integer",
                    "description": "If there is a problem, the fault will be cleared after this many hours",
                    "default": 24,
                    "minimum": 0
                },
                "send_error_notifications": {
                    "title": "Send Error Notifications",
                    "x-name": "send_error_notifications",
                    "x-hidden": false,
                    "type": "boolean",
                    "description": "Whether to send error notifications",
                    "default": true
                }
            },
            "additionalElements": true,
            "required": [
                "run_sense_pins"
            ]
        },
        "owner_org": "36a22e32-ab81-4a39-b400-869d532a052d",
        "container_registry_profile": "88953909-e9c4-4f81-9ec0-56239d4b0a2e",
        "key": "f89308d1-3969-4eec-a23a-ab98c833d89d",
        "repo_branch": "main",
        "staging_config": {}
    }
}